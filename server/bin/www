const app = require("../app");
const db = require("../db");

const { PORT } = process.env;

db()//
  .then(() => {//
    const serverMessage = `server started running at http://localhost:${PORT}`;

    const server = app.listen(PORT, console.log(serverMessage));
  })//
  .catch((e) => {//
    console.log(e);//
  });//
