  "paths": {
    "/api/members/checkin": {
      "post": {
        "summary": "new user account creation",
        "produces": [],
        "description": "create a new account",
        "parameters": [
          {
            "in": "body",
            "name": "Create Account",
            "required": true,
            "schema": {
              "description": "Enter your details",
              "title": "user details",
              "properties": {
                "firstName": {
                  "type": "string",
                  "default": "Jane"
                },
                "lastName": {
                  "type": "string",
                  "default": "Doe"
                },
                "dateOfBirth": {
                  "type": "string",
                  "default": "1945/06/27"
                },
                "phone": {
                  "type": "string",
                  "default": "62589662022"
                },
                "email": {
                  "type": "string",
                  "default": "jane.doe@nowhere.here"
                },
                "username": {
                  "type": "string",
                  "default": "jane_doe"
                },
                "password": {
                  "type": "string",
                  "default": "passmeinnow"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/api/members/login": {
      "post": {
        "summary": "login to the dashboard",
        "description": "login using username & password",
        "parameters": [
          {
            "in": "body",
            "name": "login",
            "required": true,
            "schema": {
              "title": "login here",
              "description": "login using your username & password",
              "properties": {
                "username": {
                  "type": "string",
                  "default": "username"
                },
                "password": {
                  "type": "string",
                  "default": "password"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }
        }
      }
    },
    "/api/members/all": {
      "get": {
        "summary": "get all registered members",
        "description": "find registered members",
        "parameters": [],
        "responses": {
          "200": {
            "description": ""
          }
        }
      }
    },
    "/api/members/profile/{userID}": {
      "get": {
        "summary": "member profile",
        "description": "fetch a particular member profile",
        "parameters": [
          {
            "in": "path",
            "name": "userID",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "profile details"
          }
        }
      }
    },
    "/api/reviews/{userID}/new": {
      "post": {
        "summary": "add a review",
        "description": "give review to other users",
        "parameters": [
          {
            "in": "path",
            "name": "userID",
            "type": "string",
            "required": true
          },
          {
            "in": "body",
            "name": "review",
            "schema": {
              "properties": {
                "feedback": {
                  "type": "string",
                  "default": "very good"
                },
                "stars": {
                  "type": "number",
                  "default": 5
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "review details"
          }
        }
      }
    }
  }
}
